#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/node_modules:/mnt/c/Users/jonfo/Documents/node_modules:/mnt/c/Users/jonfo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jonfo/documents/github/@new-world/types-datasheets/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/typescript@5.0.4/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules/.pnpm/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/types-datasheets/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/@new-world/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/node_modules:/mnt/c/Users/jonfo/Documents/node_modules:/mnt/c/Users/jonfo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jonfo/documents/github/@new-world/types-datasheets/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
